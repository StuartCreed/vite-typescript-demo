{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "noEmit": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "importHelpers": true,
    "plugins": [{"name": "typescript-tslint-plugin"}]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
}
